<%- model_class = DetalleEvaluacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:evaluacion_id) %>:</strong></dt>
  <dd><%= @detalle_evaluacion.evaluacion_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:factor_id) %>:</strong></dt>
  <dd><%= @detalle_evaluacion.factor_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nota) %>:</strong></dt>
  <dd><%= @detalle_evaluacion.nota %></dd>
  <dt><strong><%= model_class.human_attribute_name(:observaciones) %>:</strong></dt>
  <dd><%= @detalle_evaluacion.observaciones %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              detalle_evaluaciones_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_detalle_evaluacion_path(@detalle_evaluacion), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              detalle_evaluacion_path(@detalle_evaluacion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
