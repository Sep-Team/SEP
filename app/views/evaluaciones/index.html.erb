<div id="example" class="page-header">
    <h3>Datos Evaluaciones</h3>
    <div class="navbar">
    <div class="navbar-inner">
    <li id='buscar' class="brand" href="#">Buscar</li>
      <div class="row">
        <div class="span6">
          <form method="GET" class="form-inline" >
            <input id='buscar' name="buscar" class="search-query" type="text" placeholder="Buscar" >
            <button id='buscar' type="submit" class="btn btn-primary"><i class="icon-search icon-white"></i></button>
          </form>
        </div>
      </div>
    </div>
  </div>
  </div>
  <table class="table table-hover"  >
    <thead>
      <tr>
        <th >id</th>
        <th >Estudiantes</th>
        <th >Jefes</th> 
        <th >FechaInicio</th>
        <th >FechaEvaluación</th>
        <th >AspectosPositivos</th> 
        <th >AspectosNegativos</th>
        <th >FechaFin</th> 
        <th >Acciones</th>          
      </tr>
    </thead>
    <tbody>
      <%= render @evaluaciones %>
    </tbody>
    <tfoot>
      <tr>
        <td>
          <%= link_to image_tag('Crud/save.png'),  new_evaluacion_path,  :remote => true %>
        </td>
        <td>
         <%= link_to image_tag("Crud/admin.png"), admin_path,  :rel => "tooltip", :title => "Regresar Administración", :class => "btn btn-mini"  %></td>
        </td>
      </tr>
      </tfoot>
  </table>
  <div id="pagination">
    <ul class="pagination">
      <%= will_paginate @evaluaciones, :previous_label => '&laquo; Anterior', :next_label => 'Siguiente&raquo;' %>
    </ul>
  </div>
      