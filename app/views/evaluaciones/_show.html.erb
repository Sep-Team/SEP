<div class="modal-header">
  <button  class="close" onclick="$.lazybox.close()">&times;</button>
  <h3>Evaluaciones</h3>
</div>
  <div class="modal-body">
    <table class="table table-striped table-condensed">
      <tr>
        <td><b class="pull-right">ID: </b></td>
        <td><%= @evaluacion.id %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Estudiante: </b></td>
        <td><%= @evaluacion.estudiante.nombre1 %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Jefes: </b></td>
        <td><%= @evaluacion.jefe.nombres %></td>
      </tr>
      <tr>
        <td><b class="pull-right">FechaInicio: </b></td>
        <td><%= @evaluacion.fecha_inicio %></td>
      </tr>
      <tr>
        <td><b class="pull-right">FechaEvaluacion: </b></td>
        <td><%= @evaluacion.fecha_evaluacion %></td>
      </tr>
      <tr>
        <td><b class="pull-right">AspectosPositivos: </b></td>
        <td><%= @evaluacion.aspectos_positivos %></td>
      </tr>
       <tr>
        <td><b class="pull-right">AspectosNegativos: </b></td>
        <td><%= @evaluacion.aspectos_negativos %></td>
      </tr>
      <tr>
        <td><b class="pull-right">FechaFin: </b></td>
        <td><%= @evaluacion.fecha_fin %></td>
      </tr>
    </table>
    <div class="modal-footer">
      <a onclick="$.lazybox.close()">&times;</a>
    </div>           
</div>


