<div class="modal-header">
  <button  class="close" onclick="$.lazybox.close()">&times;</button>
  <h3>Estudiantes</h3>
</div>
  <div class="modal-body">
    <table class="table table-striped table-condensed">
      <tr>
        <td><b class="pull-right">ID: </b></td>
        <td><%= @estudiante.id %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Primer Nombre: </b></td>
        <td><%= @estudiante.nombre1 %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Segundo Nombre: </b></td>
        <td><%= @estudiante.nombre2 %></td>
      </tr>
       <tr>
        <td><b class="pull-right">PrimerApellido: </b></td>
        <td><%= @estudiante.apellido1 %></td>
      </tr>
      <tr>
        <td><b class="pull-right">SegundoApellido: </b></td>
        <td><%= @estudiante.apellido2 %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Genero: </b></td>
        <td><%= @estudiante.genero %></td>
      </tr>
      <tr>
        <td><b class="pull-right">Email: </b></td>
        <td><%= @estudiante.email %></td>
      </tr>
       <tr>
        <td><b class="pull-right">TipoDocumento: </b></td>
        <td><%= @estudiante.tipodoc_id %></td>
      </tr>
      <tr>
        <td><b class="pull-right">#Documento: </b></td>
        <td><%= @estudiante.documento %></td>
      </tr>
       <tr>
        <td><b class="pull-right">Fichas: </b></td>
        <td><%= @estudiante.ficha_id %></td>
      </tr>
       <tr>
        <td><b class="pull-right">Jefes: </b></td>
        <td><%= @estudiante.jefe_id %></td>
      </tr>
    </table>
    <div class="modal-footer">
      <a onclick="$.lazybox.close()">&times;</a>
    </div>           
</div>


