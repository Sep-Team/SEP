<%- model_class = AsignarProy -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:estudiante_id) %></th>
      <th><%= model_class.human_attribute_name(:objsyproy_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @asignares_proy.each do |asignar_proy| %>
      <tr>
        <td><%= link_to asignar_proy.id, asignar_proy_path(asignar_proy) %></td>
        <td><%= asignar_proy.estudiante_id %></td>
        <td><%= asignar_proy.objsyproy_id %></td>
        <td><%=l asignar_proy.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_asignar_proy_path(asignar_proy), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      asignar_proy_path(asignar_proy),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_asignar_proy_path,
            :class => 'btn btn-primary' %>
